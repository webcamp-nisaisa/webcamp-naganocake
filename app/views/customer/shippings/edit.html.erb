<div class="p-5">
  <h4 class="mt-5">配送先編集</h4>
  <%= form_with model:@shipping,local:true do |f| %>
    <%= f.label:"郵便番号（ハイフンなし）" %>
    <%= f.number_field:post_code %>
    <%= f.label:"住所" %>
    <%= f.text_field:address %>
    <%= f.label:"宛名" %>
    <%= f.text_field:name %>
    <%= f.submit "変更内容を保存",class:"btn btn-success" %>
  <% end %>
</div>